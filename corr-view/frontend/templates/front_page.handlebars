<div id="account-modal" class="modal login-form">
  <div class="modal-content">
    <div class="row">
      <div class="input-field col s12 center">
        <img src="images/picture.png" id="account-user-picture" alt="" class="circle responsive-img valign profile-image-login">
      </div>
    </div>
    <div class="divider"></div>
    <div class="row margin">
      <div class="input-field col s12">
        <i class="mdi-communication-email prefix"></i>
        <input readonly id="view-email" type="email">
        <label id="view-email-value" for="view-email">Email</label>
      </div>
    </div>
    <div class="row margin">
      <div class="input-field col s12">
        <i class="mdi-communication-vpn-key prefix"></i>
        <input readonly id="view-api" type="text">
        <label class"truncate" id="view-api-value" for="view-api">Api Key</label>
      </div>
    </div>
    <div class="row margin">
      <div class="input-field col s12">
        <button data-clipboard-action="copy" data-clipboard-target="#view-api" class="btn waves-effect waves-light col s12">Copy Token</button>
      </div>
    </div>
    <div class="row margin">
      <div class="input-field col s12">
        <a href="#account-update-modal" class="modal-trigger btn waves-effect waves-light col s12">Update Account</a>
      </div>
    </div>
  </div>
</div>

<div id="account-update-modal" class="modal login-form">
  <div class="modal-content">
    <div class="row center tooltipped" data-position="top" data-delay="50" data-tooltip="click to change">
      <label for="picture-input">
        <img src="images/picture.png" alt="" id="update-user-picture" class="circle responsive-img valign profile-image-login">
      </label>
      <input id="picture-input" type="file" style="display: none;"/>
    </div>
    <div class="row margin">
      <div class="input-field col s12">
        <i class="mdi-action-lock-outline prefix"></i>
        <input id="edit-new-password" type="password">
        <label for="edit-new-password">New Password</label>
      </div>
    </div>
    <div class="row margin">
      <div class="input-field col s12">
        <i class="mdi-action-lock-outline prefix"></i>
        <input id="edit-new-password-again" type="password">
        <label for="edit-new-password-again">New Password Again</label>
      </div>
    </div>
    <div class="row margin">
      <div class="input-field col s12">
        <a onclick="return user.update();" class="btn waves-effect waves-light col s12">Confirm</a>
      </div>
    </div>
  </div>
</div>

