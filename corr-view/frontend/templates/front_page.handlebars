                <i class="mdi-communication-email prefix"></i>
                <input id="register-email" type="email" onkeypress="registerKeyPress(event);">
                <label for="register-email" class="center-align">Email</label>
              </div>
            </div>
            <div class="row margin">
              <div class="input-field col s12">
                <i class="mdi-action-lock-outline prefix"></i>
                <input id="register-password" type="password" onkeypress="registerKeyPress(event);">
                <label for="register-password">Password</label>
              </div>
            </div>
            <div class="row margin">
              <div class="input-field col s12">
                <i class="mdi-action-lock-outline prefix"></i>
                <input id="register-password-again" type="password" onkeypress="registerKeyPress(event);">
                <label for="register-password-again">Password again</label>
              </div>
            </div>
            <div class="row">
              <div class="input-field col s12">
                <a id="buttonRegister" onclick="return user.register();" class="btn waves-effect waves-light blue col s12">Register Now</a>
              </div>
            </div>
          </div>
        </li>
        <li>
          <div class="collapsible-header teal white-text"><i class="mdi-communication-vpn-key"></i>Forgot Password</div>
          <div class="collapsible-body recent-activity">
